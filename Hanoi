import sys
sys.setrecursionlimit(10000)
N = (int(input("Введите количество дисков:")))
a = []
b = []
c = []
for i in range(1,N+1):
    a.append(i)
a.reverse()
print(a)


def perenos(n1, n2):
    n2.append(n1[(len(n1) - 1)])
    return print(n1, "->", "[0]", "=", "[0]", n2)


def hanoi(a, b, c, N):
    if N == 1:
        return perenos(a, c)
    else:
        hanoi(a, b, c, N-1)
        perenos(a, c)
        hanoi(b, a, c, N-1)


print(hanoi(a, b, c, N))






